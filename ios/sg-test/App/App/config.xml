<?xml version='1.0' encoding='utf-8'?>
<widget version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <access origin="*" />
  
  <feature name="SecureWebView">
    <param name="ios-package" value="SecureWebView"/>
  </feature>
  <feature name="IonicHttp">
    <param name="ios-package" value="IonicHttp"/>
  </feature>
  <feature name="IonicAuthConnectStorage">
    <param name="ios-package" value="IonicAuthConnectStorage"/>
  </feature>

  <feature name="IonicNativeAuth">
    <param name="ios-package" value="IonicNativeAuthPlugin" onload="true"/>
  </feature>

  <feature name="CDVOrientation">
    <param name="ios-package" value="CDVOrientation"/>
  </feature>

  <feature name="CordovaHttpPlugin">
    <param name="ios-package" value="CordovaHttpPlugin"/>
  </feature>

  <feature name="AppVersion">
    <param name="ios-package" value="AppVersion"/>
  </feature>

  <feature name="Contacts">
    <param name="ios-package" value="CDVContacts"/>
  </feature>

  <feature name="Device">
    <param name="ios-package" value="CDVDevice"/>
  </feature>

  <feature name="File">
    <param name="ios-package" value="CDVFile"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="FileOpener2">
    <param name="ios-package" value="FileOpener2"/>
  </feature>

  <feature name="Fingerprint">
    <param name="ios-package" value="Fingerprint"/>
  </feature>

  <feature name="QRScanner">
    <param name="ios-package" value="QRScanner"/>
  </feature>

  
  <preference name="URL_SCHEME" value="com.simedarbymotors.appauth://" />
</widget>